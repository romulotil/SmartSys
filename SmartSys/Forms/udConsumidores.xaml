<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="SmartSys.udConsumidores"
	x:Name="UserControl"
	d:DesignWidth="640" d:DesignHeight="480">
	<DockPanel Margin="0">
		<Grid Margin="0" Width="70" HorizontalAlignment="Left">
			<Button Content="Button" Height="60" Margin="10,10,0,10" VerticalAlignment="Top" Width="60"/>
			<Button Content="Button" Height="60" Margin="10,80,0,0" VerticalAlignment="Top" Width="60"/>
			<Button Content="Button" Height="60" Margin="10,150,0,0" VerticalAlignment="Top" Width="60"/>
			<Button Content="Button" Margin="10,220,0,0" Width="60" Height="60" VerticalAlignment="Top"/>
		</Grid>
		<Grid Margin="0">
			<DataGrid Margin="10"/>
		</Grid>
	</DockPanel>
</UserControl>